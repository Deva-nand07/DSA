class Solution {
public:
    vector<double> convertTemperature(double celsius) {
        double Kelvin, Fahrenheit;
        vector<double> ans;
        Kelvin = celsius + 273.15;
        ans.push_back(Kelvin);
        Fahrenheit = celsius * 1.80 + 32.00;
        ans.push_back(Fahrenheit);
        return ans;
    }
};
